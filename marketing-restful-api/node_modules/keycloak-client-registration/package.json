    "istanbul": "^0.4.3",
    "jsdoc": "^3.4.0",
    "jshint": "^2.9.1",
    "nsp": "^2.3.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1"
  },
  "dependencies": {
    "request": "^2.72.0"
  }
}
